$myFonts: 'Playfair Display', serif

body
  background : white

.container
  position: absolute
  top: 20%
  left: 50%
  margin-right: -50%
  transform: translate(-50%, -50%)


h1
  font-family: 100% $myFonts
  font-size: 2.0rem

h3
  font-family: 100% $myFonts
  font-weight: 700

p
  font-family: 100% $myFonts
  font-weight: 400 italic
